doctype html
html
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/style.css')
    
    // Import Google Icon Font
    link(href='http://fonts.googleapis.com/icon?family=Material+Icons', rel='stylesheet')
    
    // Import materialize.css
    link(type='text/css', rel='stylesheet', href='/materialize/css/materialize.min.css', media='screen,projection')
    
    // Let browser know website is optimized for mobile
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
   
    // Import jQuery before materialize.js
    script(type='text/javascript', src='https://code.jquery.com/jquery-2.1.1.min.js')
    script(type='text/javascript', src='/materialize/js/materialize.min.js')
    
    script(async='', defer='', src='https://maps.googleapis.com/maps/api/js?key=AIzaSyA5OP_7X4YFiiuTdYU_y-ThydnkKWvHgrk&callback=initMap')
    
    script(type='text/javascript', src='/javascripts/maps.js')
    script(type='text/javascript', src='/javascripts/contacts.js')
    script(type='text/javascript', src='/javascripts/mailer.js')
    script(type='text/javascript', src='/javascripts/navbar.js')
    
    //font-awesome icons
    script(src='https://use.fontawesome.com/ff80a93b3c.js')
  body(style="padding:0px !important;")
    
    include navbar
    block content
